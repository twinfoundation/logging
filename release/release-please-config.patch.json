{
	"$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json",
	"description": "Auto-Generated Release Please configuration for patch",
	"pull-request-title-pattern": "chore: patch release prepared",
	"pull-request-header": "chore: patch release prepared",
	"pull-request-footer": "This PR was generated by the prepare-release GHA",
	"release-type": "node",
	"versioning": "always-bump-patch",
	"packages": {
		"packages/logging-models": {
			"package-name": "logging-models",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/index.ts"]
		},
		"packages/logging-connector-console": {
			"package-name": "logging-connector-console",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/index.ts"]
		},
		"packages/logging-connector-entity-storage": {
			"package-name": "logging-connector-entity-storage",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/index.ts"]
		},
		"packages/logging-service": {
			"package-name": "logging-service",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/index.ts"]
		},
		"packages/logging-rest-client": {
			"package-name": "logging-rest-client",
			"changelog-path": "docs/changelog.md",
			"extra-files": ["src/index.ts"]
		}
	},
	"plugins": [
		"node-workspace",
		{
			"type": "linked-versions",
			"groupName": "repo",
			"components": [
				"logging-models",
				"logging-connector-console",
				"logging-connector-entity-storage",
				"logging-service",
				"logging-rest-client"
			]
		}
	]
}
